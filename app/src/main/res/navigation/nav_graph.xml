<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.mezda.aciud.ui.login.LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_searchFragment"
            app:destination="@id/searchFragment" />

    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.mezda.aciud.ui.register.RegisterFragment" />
    <fragment
        android:id="@+id/liftingFragment"
        android:name="com.mezda.aciud.ui.lifting.LiftingFragment" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.mezda.aciud.ui.search.SearchFragment" >
        <action
            android:id="@+id/action_searchFragment_to_liftingFragment"
            app:destination="@id/liftingFragment" />
        <action
            android:id="@+id/action_searchFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_searchFragment_to_userInfoFragment"
            app:destination="@id/liftingGraph" >
            <argument
                android:name="lifting"
                app:argType="com.mezda.aciud.data.models.LiftingInfo"
                app:nullable="true"
                android:defaultValue="@null"/>
        </action>
        <action
            android:id="@+id/action_searchFragment_to_usersFragment"
            app:destination="@id/usersFragment" />
        <action
            android:id="@+id/action_searchFragment_to_supportFragment"
            app:destination="@id/nav_support">
            <argument
                android:name="IdLift"
                android:defaultValue="0"
                app:argType="integer" />
        </action>
        <action
            android:id="@+id/action_searchFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.mezda.aciud.ui.map.MapsFragment"
        android:label="@string/location" >
        <argument
            android:name="locations"
            app:argType="com.mezda.aciud.data.models.Locations" />
    </fragment>
    <navigation android:id="@+id/liftingGraph"
        app:startDestination="@id/userInfoFragment">
        <fragment
            android:id="@+id/userInfoFragment"
            android:name="com.mezda.aciud.ui.lifting_flow.user_info.UserInfoFragment"
            android:label="Levantamiento Paso 1" >
            <action
                android:id="@+id/action_userInfoFragment_to_addressInfoFragment"
                app:destination="@id/addressInfoFragment" />
            <argument
                android:name="lifting"
                app:argType="com.mezda.aciud.data.models.LiftingInfo"
                app:nullable="true" />
        </fragment>
        <fragment
            android:id="@+id/addressInfoFragment"
            android:name="com.mezda.aciud.ui.lifting_flow.address_info.AddressInfoFragment"
            android:label="Levantamiento Paso 2" >
            <action
                android:id="@+id/action_addressInfoFragment_to_geoLocationInfoFragment"
                app:destination="@id/geoLocationInfoFragment" />
        </fragment>
        <fragment
            android:id="@+id/geoLocationInfoFragment"
            android:name="com.mezda.aciud.ui.lifting_flow.geolocation_info.GeoLocationInfoFragment"
            android:label="Levantamiento Paso 3" >
            <action
                android:id="@+id/action_geoLocationInfoFragment_to_occupationInfoFragment"
                app:destination="@id/occupationInfoFragment" />
        </fragment>
        <fragment
            android:id="@+id/occupationInfoFragment"
            android:name="com.mezda.aciud.ui.lifting_flow.ocupation_info.OccupationInfoFragment"
            android:label="Levantamiento Paso 4" >
            <action
                android:id="@+id/action_occupationInfoFragment_to_previewInfoFragment"
                app:destination="@id/previewInfoFragment" />
        </fragment>
        <fragment
            android:id="@+id/previewInfoFragment"
            android:name="com.mezda.aciud.ui.lifting_flow.preview_info.PreviewInfoFragment"
            android:label="Datos Recolectados" >
            <action
                android:id="@+id/action_partyInfoFragment_to_searchFragment"
                app:destination="@id/searchFragment"
                app:popUpTo="@id/searchFragment" />
        </fragment>
    </navigation>
    <fragment
        android:id="@+id/usersFragment"
        android:name="com.mezda.aciud.ui.users.UsersFragment"
        android:label="Cuentas" />
    <navigation android:id="@+id/nav_support"
        app:startDestination="@id/supportFragment">
        <fragment
            android:id="@+id/supportFragment"
            android:name="com.mezda.aciud.ui.support.list.SupportListFragment"
            android:label="Apoyos" >
            <argument
                android:name="IdLift"
                android:defaultValue="0"
                app:argType="integer" />
            <action
                android:id="@+id/action_supportFragment_to_supportNewFragment"
                app:destination="@id/supportNewFragment" />
            <action
                android:id="@+id/action_supportFragment_to_supportImageFragment"
                app:destination="@id/supportImageFragment" />
        </fragment>
        <fragment
            android:id="@+id/supportNewFragment"
            android:name="com.mezda.aciud.ui.support.create.SupportNewFragment"
            android:label="Agregar Apoyo" />
        <fragment
            android:id="@+id/supportImageFragment"
            android:name="com.mezda.aciud.ui.support.image.SupportImageFragment"
            android:label="Imagen de Apoyo" >
            <argument
                android:name="imagePath"
                app:argType="string" />
        </fragment>
    </navigation>
    <fragment
        android:id="@+id/infoFragment"
        android:name="com.mezda.aciud.ui.info.InfoFragment"
        android:label="Datos Recolectados" >
        <argument
            android:name="liftingId"
            app:argType="integer" />
    </fragment>
</navigation>